{
  "name": "CandySfdcHttpSinkConnector",
  "config": {
    "connector.class": "io.confluent.connect.http.HttpSinkConnector",
    "tasks.max": "1",
    "value.converter": "io.confluent.connect.avro.AvroConverter",
    "errors.tolerance": "all",
    "errors.log.enable": "true",
    "errors.log.include.messages": "true",
    "topics": "tef.de.CIM.TroubleTicket.Inbound-SFS-v1",
    "errors.deadletterqueue.topic.name": "cim-mm.CIM.TroubleTicket.Inbound-SFS-v1.dlq",
    "errors.deadletterqueue.context.headers.enable": "true",
    "http.api.url": "${file:/mnt/secrets/connector-creds/cim_mm_sfdc.properties:HTTP_API_URL}",
    "request.method": "POST",
    "headers": "Content-Type:application/json|Accept:application/json",
    "header.separator": "|",
    "http.proxy.host": "BC-PROXY-VIP-PROD.de.pri.o2.com",
    "http.proxy.port": "8080",
    "auth.type": "oauth2",
    "oauth2.token.url": "${file:/mnt/secrets/connector-creds/cim_mm_sfdc.properties:OAUTH2_TOKEN_URL}",
    "oauth2.client.id": "${file:/mnt/secrets/connector-creds/cim_mm_sfdc.properties:SALESFORCE_CONSUMER_KEY}",
    "oauth2.client.secret": "${file:/mnt/secrets/connector-creds/cim_mm_sfdc.properties:SALESFORCE_CONSUMER_SECRET}",
    "oauth2.client.scope": "",
    "max.retries": "3",
    "request.body.format": "json",
    "batch.json.as.array": "false",
    "https.ssl.key.password": "${file:/mnt/secrets/connector-creds/cim-mm-connector.passphrase:passphrase}",
    "https.ssl.keystore.location": "/mnt/secrets/connector-creds/cim-mm-connector-keystore.p12",
    "https.ssl.keystore.password": "${file:/mnt/secrets/connector-creds/cim-mm-connector.passphrase:passphrase}",
    "https.ssl.truststore.location": "/mnt/secrets/connector-creds/http-truststore.p12",
    "https.ssl.truststore.password": "${file:/mnt/secrets/connector-creds/http.passphrase:passphrase}",
    "https.ssl.protocol": "TLSv1.2",
    "reporter.result.topic.name": "cim-mm.CIM.TroubleTicket.Inbound-SFS-v1-success",
    "reporter.result.topic.replication.factor": "3",
    "reporter.error.topic.name": "cim-mm.CIM.TroubleTicket.Inbound-SFS-v1-error",
    "reporter.error.topic.replication.factor": "3",
    "reporter.bootstrap.servers": "kafka-thales.ffm-prod.svc.cluster.local:9071",
    "consumer.override.ssl.keystore.type": "PKCS12",
    "consumer.override.ssl.truststore.location": "/mnt/sslcerts/truststore.p12",
    "consumer.override.ssl.keystore.password": "${file:/mnt/secrets/connector-creds/cim-mm-connector.passphrase:passphrase}",
    "consumer.override.ssl.key.password": "${file:/mnt/secrets/connector-creds/cim-mm-connector.passphrase:passphrase}",
    "consumer.override.group.id": "cim-mm-connector-CandySfdcHttpSinkConnector",
    "consumer.override.ssl.keystore.location": "/mnt/secrets/connector-creds/cim-mm-connector-keystore.p12",
    "consumer.override.ssl.truststore.password": "${file:/mnt/sslcerts/jksPassword.txt:jksPassword}",
    "consumer.override.ssl.truststore.type": "PKCS12",
    "value.converter.schema.registry.ssl.key.password": "${file:/mnt/secrets/connector-creds/cim-mm-connector.passphrase:passphrase}",
    "kafka.topic.lowercase": "false",
    "value.converter.schema.registry.ssl.keystore.password": "${file:/mnt/secrets/connector-creds/cim-mm-connector.passphrase:passphrase}",
    "reporter.admin.ssl.key.password": "${file:/mnt/secrets/connector-creds/cim-mm-connector.passphrase:passphrase}",
    "reporter.producer.ssl.keystore.location": "/mnt/secrets/connector-creds/cim-mm-connector-keystore.p12",
    "reporter.admin.ssl.truststore.location": "/mnt/sslcerts/truststore.p12",
    "key.converter.schemas.enable": "false",
    "value.converter.schema.registry.url": "https://schemaregistry-thales.ffm-prod.svc.cluster.local:8081",
    "reporter.admin.ssl.keystore.password": "${file:/mnt/secrets/connector-creds/cim-mm-connector.passphrase:passphrase}",
    "reporter.producer.ssl.truststore.location": "/mnt/sslcerts/truststore.p12",
    "value.deserializer.encoding": "UTF_8",
    "value.converter.schemas.enable": "true",
    "reporter.admin.ssl.keystore.location": "/mnt/secrets/connector-creds/cim-mm-connector-keystore.p12",
    "value.converter.schema.registry.ssl.truststore.password": "${file:/mnt/sslcerts/jksPassword.txt:jksPassword}",
    "value.converter.schema.registry.ssl.keystore.location": "/mnt/secrets/connector-creds/cim-mm-connector-keystore.p12",
    "reporter.admin.ssl.truststore.password": "${file:/mnt/sslcerts/jksPassword.txt:jksPassword}",
    "reporter.producer.ssl.truststore.password": "${file:/mnt/sslcerts/jksPassword.txt:jksPassword}",
    "reporter.producer.ssl.keystore.password": "${file:/mnt/secrets/connector-creds/cim-mm-connector.passphrase:passphrase}",
    "reporter.admin.security.protocol": "SSL",
    "value.converter.schema.registry.ssl.truststore.location": "/mnt/sslcerts/truststore.p12",
    "reporter.producer.security.protocol": "SSL",
    "reporter.producer.ssl.key.password": "${file:/mnt/secrets/connector-creds/cim-mm-connector.passphrase:passphrase}"
  }
}