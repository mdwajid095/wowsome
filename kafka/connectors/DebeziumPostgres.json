{
"name": "CC-DECISION-DDC-Source",
"config": {
"connector.class": "io.debezium.connector.postgresql.PostgresConnector",
"tasks.max": "1",
"key.converter": "io.confluent.connect.avro.AvroConverter",
"value.converter": "io.confluent.connect.avro.AvroConverter",
"topic.creation.enable" : "true",
"topic.creation.default.replication.factor" :"3",
"topic.creation.default.partitions":"3",
"transforms": "addTopicSuffix",
"transforms.addTopicSuffix.type": "org.apache.kafka.connect.transforms.RegexRouter",
"transforms.addTopicSuffix.regex": "(.*)",
"transforms.addTopicSuffix.replacement": "$1-v1",
"topic.prefix": "tef.de.risk.cdc",
"database.hostname": "${file:/mnt/sslcerts/risk_db_cred.properties:CC_DB_IP}",
"database.port": "${file:/mnt/sslcerts/risk_db_cred.properties:CC_DB_PORT}",
"database.user": "${file:/mnt/sslcerts/risk_db_cred.properties:CC_DB_USER}",
"database.password": "${file:/mnt/sslcerts/risk_db_cred.properties:CC_DB_PASS}",
"database.dbname": "${file:/mnt/sslcerts/risk_db_cred.properties:CC_DB_NAME}",
"database.schema": "decision",
"database.sslmode": "verify-ca",
"database.sslrootcert": "/mnt/sslcerts/server-ca-test.pem",
"database.sslkey": "/mnt/sslcerts/client-key-test.pk8",
"database.sslcert": "/mnt/sslcerts/client-cert-test.pem",
"plugin.name": "pgoutput",
"slot.name": "slot_decision_ddc_ict_v1",
"publication.name": "dbz_publication",
"snapshot.mode": "never",
"signal.enabled.channels": "source, kafka",
"signal.data.collection": "ddc_debezium.debezium_signal",
"signal.kafka.topic": "risk.debezium-signal",
"signal.kafka.bootstrap.servers": "${file:/mnt/sslcerts/risk_db_cred.properties:EMP_BOOTSTRAP_URL}",
"notification.enabled.channels": "source",
"notification.sink.topic.name": "tef.de.risk.cdc.DebeziumNotification",
"table.include.list":"decision.checker,decision.finalsteps,decision.reasoncodes,decision.resultcodes",
"signal.consumer.security.protocol":"SSL",
"signal.kafka.groupId": "risk.kafka-signal",
"signal.consumer.ssl.keystore.location": "/mnt/sslcerts/keystore.jks",
"signal.consumer.ssl.key.password": "mystorepassword",
"signal.consumer.ssl.keystore.password": "mystorepassword",
"signal.consumer.ssl.truststore.password": "mystorepassword",
"signal.consumer.ssl.truststore.location": "/mnt/sslcerts/truststore.jks",
"signal.consumer.ssl.keystore.location": "/mnt/sslcerts/keystore.jks",
"signal.consumer.ssl.keystore.password": "mystorepassword",
"signal.consumer.ssl.truststore.location": "/mnt/sslcerts/truststore.jks",
"signal.consumer.ssl.truststore.password": "mystorepassword",
"signal.consumer.ssl.key.password": "mystorepassword",
"value.converter.connect.meta.data" : "false",
"value.converter.enhanced.avro.schema.support" : "true",
"value.converter.latest.compatibility.strict" : "false",
"value.converter.ignore.default.for.nullables" : "true",
"key.converter.schema.registry.ssl.keystore.location": "/mnt/sslcerts/keystore.jks",
"value.converter.schema.registry.ssl.key.password": "mystorepassword",
"value.converter.schema.registry.ssl.keystore.password": "mystorepassword",
"value.converter.schema.registry.ssl.truststore.password": "mystorepassword",
"key.converter.schema.registry.ssl.truststore.location": "/mnt/sslcerts/truststore.jks",
"value.converter.schema.registry.ssl.keystore.location": "/mnt/sslcerts/keystore.jks",
"key.converter.schema.registry.ssl.keystore.password": "mystorepassword",
"value.converter.schema.registry.ssl.truststore.location": "/mnt/sslcerts/truststore.jks",
"key.converter.schema.registry.ssl.truststore.password": "mystorepassword",
"value.converter.schema.registry.url": "${file:/mnt/sslcerts/risk_db_cred.properties:EMP_SCHEMA_URL}",
"http.proxy": "10.169.127.8:8080",
"key.converter.schema.registry.ssl.key.password": "mystorepassword",
"key.converter.schema.registry.url": "${file:/mnt/sslcerts/risk_db_cred.properties:EMP_SCHEMA_URL}",
"producer.max.request.size": 60886080,
"heartbeat.interval.ms":"10000",
"slot.retry.delay.ms":"300000",
"topic.heartbeat.prefix":"risk.cdc.heartbeat.decision-Debezium"
}
}

